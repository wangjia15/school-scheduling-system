{
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.completion.importOrder": [
    "java",
    "javax",
    "org",
    "com"
  ],
  "java.format.settings.url": ".vscode/eclipse-java-google-style.xml",
  "java.format.settings.profile": "GoogleStyle",
  "maven.terminal.customEnv": [
    {
      "environmentVariable": "JAVA_HOME",
      "value": "${env:JAVA_HOME}"
    }
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.fixAll": "explicit"
  },
  "files.associations": {
    "*.xml": "xml"
  },
  "files.exclude": {
    "**/target": true,
    "**/node_modules": true,
    "**/.mvn": true,
    "**/dist": true,
    "**/build": true,
    "**/logs": true,
    "**/.git": true
  },
  "search.exclude": {
    "**/target": true,
    "**/node_modules": true,
    "**/.mvn": true,
    "**/dist": true,
    "**/build": true,
    "**/logs": true
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "vue.codeActions.enabled": true,
  "vue.complete.casing.tags": "pascal",
  "vue.complete.casing.props": "camel",
  "vetur.validation.template": false,
  "volar.codeLens.references": true,
  "volar.codeLens.pugTools": true,
  "volar.autoCompleteRefs": true,
  "volar.validation.template": true,
  "volar.validation.script": true,
  "volar.validation.style": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "eslint.workingDirectories": [
    "./frontend"
  ],
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.defaultProfile.osx": "bash"
}